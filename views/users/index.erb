<h2>User Model</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>email</th>
      <th>address</th>
      <th>password</th>
      <th>cc_number</th>
      <th>date_joined</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%= user[:id] %>
      </td>
      <td>
        <%= user[:name] %>
      </td>
      <td>
        <%= user[:email] %>
      </td>
      <td>
        <%= user[:address] %>
      </td>
      <td>
        <%= user[:password] %>
      </td>
      <td>
        <%= user[:cc_number] %>
      </td>
      <td>
        <%= user[:date_joined] %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
